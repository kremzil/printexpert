{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "quality:gate": "npm run lint && npm run typecheck && npm run build",
    "db:seed": "prisma db seed",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker logs -f shop-db",
    "db:ps": "docker ps",
    "db:migrate:prod:status": "node scripts/db/migrate-prod.mjs status --env-file .env.production --retries 3",
    "db:migrate:prod:deploy": "node scripts/db/migrate-prod.mjs deploy --env-file .env.production --retries 3",
    "db:sync:data:dry": "node scripts/db/sync-data.mjs --from-env .env --to-env .env.production --plan scripts/db/sync-plan.json --dry-run",
    "db:sync:data:apply": "node scripts/db/sync-data.mjs --from-env .env --to-env .env.production --plan scripts/db/sync-plan.json --apply",
    "prisma:studio": "prisma studio",
    "db:reset": "docker compose down -v && docker compose up -d",
    "prod:up": "docker compose -f docker-compose.prod.yml --env-file .env.production up -d",
    "prod:down": "docker compose -f docker-compose.prod.yml --env-file .env.production down",
    "prod:logs": "docker logs -f shop-db",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "import:wp-customers": "node scripts/import-wp-customers-orders.js",
    "import:wp-customers:apply": "node scripts/import-wp-customers-orders.js --apply",
    "postinstall": "prisma generate",
    "health": "node -e \"fetch('http://localhost:3000/api/health').then(r=>r.text()).then(console.log).catch(e=>{console.error(e);process.exit(1)})\""
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
